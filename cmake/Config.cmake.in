@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

set(CMAKE_MODULE_PATH .)

find_dependency(OpenShotAudio)
find_dependency(OpenMP)
if(NOT TARGET OpenMP::OpenMP_CXX)
    # Older CMake versions (< 3.9) don't create find targets.
    add_library(OpenMP_TARGET INTERFACE)
    add_library(OpenMP::OpenMP_CXX ALIAS OpenMP_TARGET)
    target_compile_options(OpenMP_TARGET INTERFACE ${OpenMP_CXX_FLAGS})
    target_link_libraries(OpenMP_TARGET INTERFACE ${OpenMP_CXX_FLAGS})
endif()
find_dependency(FFmpeg)
find_dependency(Qt5)
find_dependency(jsoncpp)
find_dependency(Resvg)
find_dependency(ZeroMQ)

include("${CMAKE_CURRENT_LIST_DIR}/OpenShotLibTargets.cmake")

check_required_components(OpenShotLib)
